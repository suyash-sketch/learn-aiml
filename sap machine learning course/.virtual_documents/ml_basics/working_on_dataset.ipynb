import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns


df = pd.read_csv(r'dataset/solarpowergeneration.csv')
df.head()


df.shape


df.describe()


df.isnull().sum()


df.duplicated().sum()


plt.figure(figsize=(10,6))
sns.histplot(df['generated_power_kw'], bins=40, kde=True)
plt.title('Distribution of Generated Power (kW)')
plt.xlabel('Generated Power (kW)')
plt.ylabel('Frequency')
plt.show()


df[df.columns[9:18]].hist(bins=30, figsize=(15,10))
plt.show()


plt.figure(figsize=(15,30))
for num, column in enumerate(df.columns):
    if column != 'generated_power_kw':
        plt.subplot(7,3, num + 1)
        plt.scatter(df[column], df['generated_power_kw'])
        plt.xlabel(column)
        plt.ylabel('Generated Power kW')
        plt.title(f'{column} vs Generated Power')
plt.tight_layout()
plt.show()


plt.figure(figsize=(12,8))
correlation_matrix = df.corr()
sns.heatmap(correlation_matrix, annot=True, cmap='coolwarm', fmt ='.2f')
plt.title('Correlation Matrix')
plt.show()





import numpy as np
import pandas as pd
import seaborn as sns



salary_df = pd.read_csv(r'dataset/Salary_Data.csv')
salary_df.head()


salary_df.isnull().sum()


salary_df.shape



salary_df.describe()


salary_df.duplicated().sum()


sns.scatterplot(x = salary_df['YearsExperience'], y = salary_df['Salary'])


sns.regplot(x=salary_df['YearsExperience'], y=salary_df['Salary'])


x = salary_df['YearsExperience'].values.reshape(-1,1)
y = salary_df['Salary'].values


from sklearn.model_selection import train_test_split
x_train,x_test,y_train,y_test = train_test_split(x,y,test_size=0.4, random_state=25)


salary_df.shape


print(x_train.shape)
print(x_test.shape)
print(y_train.shape)
print(y_test.shape)


from sklearn.linear_model import LinearRegression
linear = LinearRegression()


linear.fit(x_train,y_train)


m = linear.coef_
m


c = linear.intercept_
c


y_pred = linear.predict(x_test)
y_pred


from sklearn.metrics import mean_squared_error, mean_absolute_error



mse = mean_squared_error(y_test, y_pred)
mse


mae = mean_absolute_error(y_test, y_pred)
mae









